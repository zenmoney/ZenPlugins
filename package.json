    "babel-plugin-lodash": "3.3.4",
    "babel-preset-react-app": "9.1.2",
    "big-integer": "1.6.47",
    "body-parser": "1.18.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cheerio": "1.0.0-rc.3",
    "deep-diff": "0.3.8",
    "elliptic": "6.5.3",
    "es6-iterator": "2.0.3",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-github": "4.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "exports-loader": "0.7.0",
    "fetch-mock": "5.12.2",
    "fs-extra": "3.0.1",
    "futoin-hkdf": "1.3.2",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "3.2.0",
    "http-proxy": "1.16.2",
    "i18next": "19.1.0",
    "iconv-lite": "0.4.18",
    "imports-loader": "0.7.1",
    "jest": "26.0.1",
    "jest-raw-loader": "1.0.1",
    "jshashes": "1.0.8",
    "ksuid": "1.2.0",
    "lodash": "4.17.15",
    "mockdate": "2.0.2",
    "moment": "2.24.0",
    "moment-timezone": "0.5.28",
    "node-forge": "0.9.1",
    "object-assign": "4.1.1",
    "pad-left": "2.1.0",
    "polyfill-crypto.getrandomvalues": "1.0.0",
    "promise": "8.0.1",
    "protobufjs": "6.8.8",
    "protobufjs-loader-webpack4": "git+https://github.com/maximtop/protobufjs-loader.git#657d9604f40c24108fd4550613f4df4eded92222",
    "react": "16.4.0",
    "react-dev-utils": "4.2.3",
    "react-dom": "16.4.0",
    "script-loader": "0.7.0",
    "set-cookie-parser": "2.2.1",
    "standard": "14.3.4",
    "strip-bom": "3.0.0",
    "typescript": "3.9.3",
    "util": "0.10.3",
    "webpack": "4.43.0",
    "webpack-dev-server": "3.10.3",
    "whatwg-fetch": "git+https://github.com/skvav/fetch.git#e232caddc2a945135e2e3bb5d3cfff666c2dc6d0",
    "xml2js": "^0.4.23"
  },
  "scripts": {
    "start": "yarn && node --http-parser=legacy --insecure-http-parser scripts/start.js",
    "host": "yarn && node scripts/host.js",
    "build": "yarn && node scripts/build.js",
    "test": "yarn && standard && eslint ./src && node scripts/test.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [],
    "setupFilesAfterEnv": [
      "<rootDir>/scripts/setupJestTestFramework.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.xml$": "jest-raw-loader",
      "^.+\\.html?$": "jest-raw-loader"
    },
    "moduleFileExtensions": [
      "js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "console",
      "ZenMoney",
      "TemporaryError",
      "InvalidPreferencesError"
    ],
    "env": [
      "jest"
    ],
    "ignore": [
      "/plugins/",
      "/src/polyfills/",
      "/src/UI/"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 Chrome version"
    ],
    "production": [
      "ios_saf 10"
    ]
  }
}
