<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
    <EditTextPreference
        key="login"
        title="Номер карты или логин"
        dialogTitle="Номер карты или логин"
        summary="||{@s}"
        positiveButtonText="ОК"
        negativeButtonText="Отмена"
        obligatory="true">
    </EditTextPreference>
    <EditTextPreference
        key="pin"
        title="5-значный пароль от приложения Сбербанк Онлайн"
        dialogTitle="5-значный пароль от приложения Сбербанк Онлайн"
        dialogMessage="Введите 5-значный пароль от мобильного приложения Сбербанк Онлайн. Если у вас не установлено приложение Сбербанка, введите 97531."
        inputType="numberPassword"
        summary="||*****"
        positiveButtonText="ОК"
        negativeButtonText="Отмена"
        obligatory="true">
    </EditTextPreference>
    <EditTextPreference
        key="startDate"
        obligatory="true"
        inputType="date"
        title="С какой даты загружать операции"
        dialogTitle="С какой даты загружать операции"
        defaultValue="2018-04-01T00:00:00.000Z"
        positiveButtonText="ОК"
        negativeButtonText="Отмена"
        summary="|startDate|{@s}">
    </EditTextPreference>
</PreferenceScreen>
