<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
    <EditTextPreference
        key="token"
		title="Токен API"
		summary="|Токен API|{@s}"
		dialogTitle="Ключ API"
        positiveButtonText="ОК"
		negativeButtonText="Отмена"
		dialogMessage="Введите токен API Модульбанка."
		obligatory="true"/>
	<EditTextPreference
		key="startDate"
		obligatory="true"
		inputType="date"
		title="С какой даты загружать операции"
		defaultValue="2018-01-01T00:00:00.000Z"
		dialogTitle="Дата начала загрузки"
		dialogMessage="Введите дату, с которой загружать операции"
		positiveButtonText="ОК"
		negativeButtonText="Отмена"
		summary="|startDate|{@s}"/>
</PreferenceScreen>
