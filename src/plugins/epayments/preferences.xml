<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
    <EditTextPreference
            key="login"
            summary="||{@s}"
            title="Номер телефона или емейл"
            dialogTitle="Номер телефона или емейл"
            negativeButtonText="Отмена"
            positiveButtonText="ОК"
            obligatory="true">
    </EditTextPreference>
    <EditTextPreference
            key="password"
            summary="||***********"
            title="Пароль от интернет-банка"
            dialogTitle="Пароль от интернет-банка"
            negativeButtonText="Отмена"
            positiveButtonText="ОК"
            inputType="textPassword"
            obligatory="true">
    </EditTextPreference>
    <ListPreference
            key="period"
            summary="|31|{@s}"
            title="За какой период загружать данные"
            dialogTitle="За какой период загружать данные"
            positiveButtonText="ОК"
            negativeButtonText="Отмена"
            entries="Квартал|Месяц|Неделя|Не загружать"
            entryValues="92|31|7|0"
            defaultValue="31">
    </ListPreference>
</PreferenceScreen>
